    "test": "ng test",
    "lint": "ng lint",
    "e2e:ci": "ng e2e",
    "e2e:local-run": "concurrently \"ng serve\" \"wait-on http-get://localhost:4200 && cypress run --config-file cypress.json\" --kill-others --success first",
    "e2e:local-watch": "concurrently \"ng serve\" \"wait-on http-get://localhost:4200 && cypress open --config-file cypress.json\" --kill-others --success first",
    "build-stats": "ng build --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "ng-high-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "ng-ci-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "fcm": "concat ./dist/ngsw-worker.js ./dist/firebase-messaging-sw.js -o ./dist/combined-worker.js",
    "proxy": "ng serve --configuration=proxy --proxyConfig=src/proxy.conf.json --host=192.168.2.209",
    "local": "ng serve --configuration=local"
  },
